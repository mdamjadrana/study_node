<% for (var i = 0; i < products.length; i++) { %>
    <div class="col-md-4">
        <div class="thumbnail">
            <img src="<%= products[i].cover %>">
            <div class="caption">
                <h3><%= products[i].title %></h3>
                <p><%= products[i].category %></p>
                <p><%= products[i].desc %></p>
                <a href="/products/single/<%= products[i]._id %>">Link</a>
            </div>
        </div>
    </div>
<% } %>